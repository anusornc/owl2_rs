  ObjectPropertyRange(ObjectProperty(<http://epcis.org/ontology/bizStep>) Class(<http://epcis.org/ontology/BusinessStep>))
  ObjectPropertyDomain(ObjectProperty(<http://epcis.org/ontology/disposition>) Class(<http://epcis.org/ontology/EPCISEvent>))
  ObjectPropertyRange(ObjectProperty(<http://epcis.org/ontology/disposition>) Class(<http://epcis.org/ontology/Disposition>))
