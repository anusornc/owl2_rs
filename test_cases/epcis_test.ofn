  SubClassOf(Class(<http://epcis.org/ontology/TransformationEvent>) Class(<http://epcis.org/ontology/EPCISEvent>))
  
  # Define property domains and ranges for events
  ObjectPropertyDomain(ObjectProperty(<http://epcis.org/ontology/epcList>) Class(<http://epcis.org/ontology/ObjectEvent>))
  ObjectPropertyRange(ObjectProperty(<http://epcis.org/ontology/epcList>) Class(<http://epcis.org/ontology/EPC>))
  ObjectPropertyDomain(ObjectProperty(<http://epcis.org/ontology/bizLocation>) Class(<http://epcis.org/ontology/EPCISEvent>))
