  DataPropertyRange(DataProperty(<http://epcis.org/ontology/quantity>) Datatype(<http://www.w3.org/2001/XMLSchema#integer>))
  
  # Define individuals and their class assertions
  ClassAssertion(Class(<http://epcis.org/ontology/EPCISEvent>) NamedIndividual(<http://example.com/objectEvent1>))
